{
  "name": "authentication-api",
  "private": true,
  "version": "1.0.0",
  "description": "A simple authentication API",
  "main": "dist/main/server.js",
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src --extensions '.js,.ts' --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/main/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules -- src/main/server.ts",
    "debug": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --inspect -- src/main/server.ts",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "type-check": "npx tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "prisma": "^4.11.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "redis": "^4.6.5",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.8.2",
    "zod": "^3.21.4"
  }
}
